        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="92dp"
        tools:layout_editor_absoluteY="233dp" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="wrap_content"
